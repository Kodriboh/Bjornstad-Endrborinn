{% extends "base.php.twig" %}

{% block title %}Welcome{% endblock %}

{% block header %}
    <header>
        <h1>Posts</h1>
    </header>
{% endblock %}

{% block main %}
    <ul>
        {% for post in posts %}
            <h2>{{ post.title }}</h2>
            <p>{{ post.content }}</p>
        {% endfor %}
    </ul>
{% endblock %}